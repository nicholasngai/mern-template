{
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "target": "es5",
    "lib": ["es2021", "dom", "dom.iterable"],
    "jsx": "react-jsx",
    "experimentalDecorators": false,
    "module": "commonjs",
    "rootDir": ".",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "../../build/client",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
