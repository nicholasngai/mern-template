{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:server": "go build -o ./build/dist/server ./cmd/server",
    "build:client": "tsc -b client/tsconfig.json && vite build client",
    "build": "yarn build:server && yarn build:client",
    "start": "cd build/dist && ./server",
    "dev:server": "nodemon -x \"go run ./cmd/server\"",
    "dev:client": "concurrently --names tsc,vite \"tsc -b -w --preserveWatchOutput client/tsconfig.json\" \"vite --clearScreen false client\"",
    "dev": "concurrently --names server,client \"yarn dev:server\" \"yarn dev:client\"",
    "lint": "eslint client",
    "clean": "rimraf build"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-react": "^1.1.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  }
}
